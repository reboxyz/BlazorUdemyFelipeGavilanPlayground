@page "/"
@page "/index"

@inject IRepository MovieInMemoryRepository

<div>
    <h3>Movie</h3>
    <MoviesList 
        Movies="movies"
    />
</div>

@code {
    private List<Movie> movies;

    protected override void OnInitialized()
    {
        movies = MovieInMemoryRepository.GetMovies();
    }    
}